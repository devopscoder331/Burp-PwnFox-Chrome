{
    "manifest_version": 3,
    "name": "Proxy Settings Editor",
    "version": "1.0",
    "permissions": [
        "proxy",
        "storage",
        "declarativeNetRequest",
        "webRequest",
        "webRequestBlocking",
        "<all_urls>"
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html",
        "default_title": "Set Proxy"
    }
}
